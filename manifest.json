{
  "manifest_version": 3,
  "name": "Tab Remaped",
  "version": "2.0.0",
  "description": "Remaps the Tab key on useful links",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
  {
    "matches": ["https://www.google.com/search*"],
    "js": ["scripts/content-google.js"]
  },
  {
    "matches": ["https://www.youtube.com/*"],
    "js": ["scripts/content-youtube.js"]
  }
]
}
